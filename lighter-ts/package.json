{
  "name": "lighter-ts",
  "version": "1.0.0",
  "description": "TypeScript SDK for Lighter",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "lighter",
    "typescript",
    "sdk",
    "api",
    "trading",
    "defi"
  ],
  "author": "elliot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elliottech/lighter-ts.git"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "ethers": "^6.8.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}